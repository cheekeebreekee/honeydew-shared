    "serverless-webpack": "^5.11.0",
    "terser-webpack-plugin": "^5.3.6",
    "tsc-files": "^1.1.3",
    "tsconfig-paths-webpack-plugin": "^3.5.2"
  },
  "lint-staged": {
    "src/**/*.{ts,js,md,yaml}": [
      "prettier --config .prettierrc.json --write ."
    ],
    "**/*.ts": [
      "tsc-files --noEmit"
    ]
  }
